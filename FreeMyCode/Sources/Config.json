{
   "Configuration":{
    "Tags": {
      "List open": "[",
      "List close": "]",
      "Starting tag": "@",
      "Node": "$node",
      "Secondary types": [
        "url",
        "name",
        "type",
        "repo"
      ],
      "FreeMyCode version": "@version :",
      "FreeMyCode header opening": "@<FreeMyCode>",
      "FreeMyCode header closing": "<FreeMyCode>@"
    },
    "Languages": [
      {
        "Extension array": [
          ".cpp",
          ".c",
          ".h",
          ".hpp",
          ".cs",
          ".js"
        ],
        "Bloc comment opening": "/*",
        "Bloc comment closing": "*/",
        "Single line comment": "//"
      },
      {
        "Extension array": [
          ".py",
          ".yaml"
        ],
          "Single line comment" : "#"
      },
      {
        "Extension array": [
          ".html",
          ".xml"
        ],
          "Bloc comment opening" : "<!--",
          "Bloc comment closing":"-->"
      },
      {
        "Extension": ".css",
        "Bloc comment opening": "/*",
        "Bloc comment closing": "*/"
      },
      {
        "Extension": ".vb",
        "Single line comment": "'"
      }
    ]
   }
}