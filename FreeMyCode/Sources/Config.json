{
   "Configuration":{
      "Tags":
         {
          "FreeMyCode version":"@version :",
          "Author": "@author :",
          "Author list opening": "@authors : < ",
          "Author list closing": " >authors@",
          "Organisation types": [
            "School",
            "Company",
            "Institution",
            "Association",
            "Independant"
          ],
          "Organisation name": "@<ORGANISATION> :",
          "Organisation website": "@<ORGANISATION> website :",
          "Project name": "@project :",
          "Project url": "@project url :",
          "Project repo": "@project repo :",
          "License": "@license :",
          "License url": "@license url :",
          "FreeMyCode header opening":"@<FreeMyCode>",
          "FreeMyCode header closing": "<FreeMyCode>@",
          "Date": "Date :",
          "Custom": "@<CUSTOM> :"
         },
    "Languages": [
      {
        "Extension array": [
          ".cpp",
          ".c",
          ".h",
          ".hpp",
          ".cs",
          ".js"
        ],
        "Bloc comment opening": "/*",
        "Bloc comment closing": "*/",
        "Single line comment": "//"
      },
      {
        "Extension array": [
          ".py",
          ".yaml"
        ],
          "Single line comment" : "#"
      },
      {
        "Extension array": [
          ".html",
          ".xml"
        ],
          "Bloc comment opening" : "<!--",
          "Bloc comment closing":"-->"
      },
      {
        "Extension": ".css",
        "Bloc comment opening": "/*",
        "Bloc comment closing": "*/"
      },
      {
        "Extension": ".vb",
        "Single line comment": "'"
      }
    ]
   }
}